{
    "version": "1.3.2",
    "description": "GUI for standard UNIX password manager",
    "homepage": "https://qtpass.org",
    "license": "GPL-3.0-only",
    "url": "https://github.com/IJHack/QtPass/releases/download/v1.3.2/qtpass-1.3.2.exe",
    "hash": "0fb9e68ce7e52e614ebb29dd9fc0b06b24f4262e9509090853e2c2991c171e72",
    "innosetup": true,
    "pre_install": [
        "ensure \"$persist_dir\" | Out-Null",
        "if (Test-Path \"$persist_dir\\qtpass.ini\") {",
        "    Copy-Item \"$persist_dir\\qtpass.ini\" \"$dir\" -ErrorAction SilentlyContinue",
        "} else {",
        "    New-Item \"$dir\\qtpass.ini\" | Out-Null",
        "}"
    ],
    "uninstaller": {
        "script": "Copy-Item \"$dir\\qtpass.ini\" \"$persist_dir\" -ErrorAction SilentlyContinue -Force"
    },
    "bin": "qtpass.exe",
    "shortcuts": [
        [
            "qtpass.exe",
            "QtPass"
        ]
    ],
    "checkver": {
        "github": "https://github.com/IJHack/QtPass",
    },
    "autoupdate": {
        "url": "https://github.com/IJHack/QtPass/releases/download/v$version/qtpass-$version.exe"
    }
}
